{
  "prereleaseBranches": ["development"],
  "changelog": {  
    "message": "string"
  },
  "shipit": {
    "prerelease": true,
    "message": "string",
    "noChangelog": false,
    "onlyGraduateWithReleaseLabel": true
  },
  "release":{
    "prerelease": false
  },
  "plugins": [
    "npm",
    ["conventional-commits", { "preset": "angular" }]
  ]
}